{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": ["error", 2],
    "max-len": ["warn", 90],
    "react/jsx-uses-react": "error",
    "no-var": "error",
    "quotes": ["warn", "double"],
    "react/prop-types": "off",
    "no-console": "warn"
  }
}
